{
  "name": "sed-codemirror",
  "version": "0.0.1",
  "description": "CodeMirror extension for sed",
  "scripts": {
    "prepare": "rollup -c",
    "pages": "esbuild gh-pages/editor.js --bundle --minify --outfile=gh-pages/index.js"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "author": "inventshah",
  "license": "ISC",
  "devDependencies": {
    "@lezer/generator": "^1.0.0",
    "rollup": "^2.75.6",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.7.4",
    "esbuild": "^0.14.46"
  },
  "dependencies": {
    "@codemirror/commands": "^6.0.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/state": "^6.0.1",
    "@codemirror/view": "^6.0.1",
    "@lezer/highlight": "^1.0.0",
    "@lezer/lr": "^1.0.0"
  }
}